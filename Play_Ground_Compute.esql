







CREATE COMPUTE MODULE Play_Ground_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN

		IF InputRoot.XMLNSC.root.hockey = 'hockey' THEN
			SET OutputLocalEnvironment.Destination.RouterList.DestinationData[1].labelName = 'hockey';
		ELSE
			SET OutputLocalEnvironment.Destination.RouterList.DestinationData[1].labelName = 'footbal';
		END IF;
		SET OutputRoot = InputRoot;
		RETURN TRUE;
	END;

END MODULE;